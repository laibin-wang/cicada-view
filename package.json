{
  "name": "cicada-view",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "cz": "cz",
    "changelog": "conventional-changelog -p angular -i GHANGELOG.md -s",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laibin-wang/cicada-view.git"
  },
  "keywords": [
    "cicada-view",
    "vue3",
    "vite"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "wanglaibin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laibin-wang/cicada-view/issues"
  },
  "homepage": "https://github.com/laibin-wang/cicada-view#readme",
  "lint-staged": {
    "*.{js,ts,jsx,tsx,vue,css}": [
      "prettier --write"
    ],
    "*.{js,ts,jsx,tsx,vue}": [
      "eslint --fix"
    ],
    "*.{css}": [
      "stylelint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "vue": "^3.2.25",
    "vue-router": "4"
  },
  "devDependencies": {
    "@cicada-design/eslint-config": "^0.1.2",
    "@cicada-design/stylelint-config": "^0.0.2",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/node": "^17.0.10",
    "@vitejs/plugin-vue": "^2.0.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.7.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.2",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-url": "^10.1.3",
    "prettier": "^2.5.1",
    "stylelint": "^14.3.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  }
}
